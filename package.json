{
  "name": "crip-vue-select",
  "version": "1.0.0",
  "description": "A Vue.js project to add customization around select input.",
  "main": "lib/crip-vue-select.js",
  "typings": "types/index.d.ts",
  "files": [
    "lib/*",
    "types/*"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=test karma start karma.conf.js",
    "test-watch": "cross-env NODE_ENV=test karma start karma.conf.js --no-single-run",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot --config build/webpack.config",
    "build-app": "cross-env NODE_ENV=production webpack --config build/webpack.config --progress --hide-modules",
    "build-lib": "cross-env NODE_ENV=production webpack --config build/webpack.config.lib --progress --hide-modules",
    "build": "npm run build-app && npm run build-lib",
    "release": "node build/release.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tahq69/vue-select.git"
  },
  "keywords": [
    "vue",
    "vuejs2",
    "crip",
    "vue-component"
  ],
  "author": "Igors Krasjukovs <tahq69@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tahq69/vue-select/issues"
  },
  "homepage": "https://github.com/tahq69/vue-select#readme",
  "devDependencies": {
    "@types/jasmine": "^2.8.2",
    "bootstrap-sass": "^3.3.7",
    "cross-env": "^3.2.4",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.9.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.6",
    "node-sass": "^4.7.2",
    "prettier": "^1.9.2",
    "sass-loader": "^6.0.6",
    "string-replace-loader": "^1.3.0",
    "style-loader": "^0.18.2",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.2",
    "url-loader": "^0.5.9",
    "vue": "^2.5.11",
    "vue-loader": "^12.2.2",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.11",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
